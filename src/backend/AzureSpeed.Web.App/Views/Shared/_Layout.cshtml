@inject Microsoft.ApplicationInsights.AspNetCore.JavaScriptSnippet JavaScriptSnippet
<!DOCTYPE html>
<html ng-app="azurespeed" ng-controller="MainController">
<head>
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <meta name="keywords" content="Azure, Microsoft Azure, Azure Speed Test, Azure Storage, Azure network latency, Azure blob Upload Speed, Azure blob download speed" />
  <meta name="description" content="Azure speed test tool. Test your network latency, download and upload speed to Azure datacenters around the world." />
  <title>@ViewBag.Title - Azure Speed Test</title>

  <link href="https://ajax.aspnetcdn.com/ajax/bootstrap/3.4.1/css/bootstrap.min.css" rel="stylesheet" />
  <environment include="Development">
    <link href="~/lib/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="~/lib/metisMenu/metisMenu.css" rel="stylesheet" />
    <link href="~/css/sb-admin-2.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" />
  </environment>
  <environment exclude="Development">
    <link href="~/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/lib/metisMenu/metisMenu.min.css" rel="stylesheet" />
    <link href="~/css/site.min.css" rel="stylesheet" />
  </environment>
  @* jQuery has to be loaded first. *@
  <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.2.4.min.js"></script>
  <script src="https://ajax.aspnetcdn.com/ajax/bootstrap/3.4.1/bootstrap.min.js"></script>
  <environment exclude="Development">
    <partial name="_Analytics" />
  </environment>
  @Html.Raw(JavaScriptSnippet.FullScript)
</head>
<body>
  <div id="wrapper">
    <!-- Navigation -->
    <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
      <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand" href="~/">AzureSpeed.com</a>
      </div>
      <!-- /.navbar-header -->
      <div class="navbar-default sidebar" role="navigation">
        <div class="sidebar-nav navbar-collapse">
          <ul class="nav" id="side-menu">
            <li>
              <a href="~/Azure/Latency"><i class="fa fa-cloud fa-fw"></i> Latency Test</a>
            </li>
            <li>
              <a href="~/Azure/RegionToRegionLatency"><i class="fa fa-cloud fa-fw"></i> Region to Region Latency</a>
            </li>
            <li>
              <a href="~/Azure/CDN"><i class="fa fa-cloud fa-fw"></i> CDN Test</a>
            </li>
            <li>
              <a href="~/Azure/Upload"><i class="fa fa-cloud-upload fa-fw"></i> Upload Speed Test</a>
            </li>
            <li>
              <a href="~/Azure/UploadLargeFile"><i class="fa fa-upload fa-fw"></i> Large File Upload Speed Test</a>
            </li>
            <li>
              <a href="~/Azure/Download"><i class="fa fa-cloud-download fa-fw"></i> Download Speed Test</a>
            </li>
            <li>
              <a href="~/Azure/PsPing"><i class="fa fa-cloud fa-fw"></i> PsPing Latency Test</a>
            </li>
            <li>
              <a href="~/Cloud/RegionFinder"><i class="fa fa-search fa-fw"></i> Cloud Region Finder</a>
            </li>
            <li>
              <a href="~/Information/AzureSovereignClouds"><i class="fa fa-book fa-fw"></i>Azure Sovereign Clouds</a>
            </li>
            <li>
              <a href="~/Information/AzureGeographies"><i class="fa fa-book fa-fw"></i>Azure Geographies</a>
            </li>
            <li>
              <a href="~/Information/AzureRegions"><i class="fa fa-book fa-fw"></i>Azure Regions</a>
            </li>
            <li>
              <a href="~/Information/AzureAvailabilityZones"><i class="fa fa-book fa-fw"></i>Azure Availability Zones</a>
            </li>
            <li>
              <a href="~/Information/AzureEnvironments"><i class="fa fa-book fa-fw"></i>Azure Environments</a>
            </li>
            <li>
              <a href="~/Information/AzureBillingMeters"><i class="fa fa-book fa-fw"></i>Azure Billing Meters</a>
            </li>
            <li>
              <a href="~/Information/AzureVMPricing"><i class="fa fa-book fa-fw"></i>Azure VM Pricing</a>
            </li>
            <li>
              <a href="~/Information/IpRange"><i class="fa fa-book fa-fw"></i>Datacenter IP Ranges</a>
            </li>
            <li>
              <a href="~/Information/References"><i class="fa fa-book fa-fw"></i>References</a>
            </li>
            <li>
              <a href="~/Azure/About"><i class="fa fa-book fa-fw"></i> About</a>
            </li>
          </ul>
          <!-- /#side-menu -->
          <div class="container">
            <a href="https://twitter.com/share?ref_src=twsrc%5Etfw" class="twitter-share-button" data-text="A great site to find out which #Azure datacenter is best for you." data-url="http://www.azurespeed.com" data-show-count="false">Tweet</a>
            <script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
          </div>
        </div>
        <!-- /.sidebar-collapse -->
      </div>
      <!-- /.navbar-static-side -->
    </nav>
    <div id="page-wrapper">
      @RenderBody()
      <environment exclude="Development">
        <partial name="_Adsense" />
      </environment>
    </div>
  </div>

  <environment include="Development">
    @* Need bootstrap.js to make bootstrap madal work *@
    <script src="~/lib/angular/angular.js"></script>
    <script src="~/lib/angular-filter/angular-filter.js"></script>
    <script src="~/lib/angular-ui-bootstrap/ui-bootstrap-tpls.js"></script>
    <script src="~/lib/checklist-model/checklist-model.js"></script>
    <script src="~/lib/angular-local-storage/angular-local-storage.js"></script>
    <script src="~/lib/d3/d3.js"></script>
    <script src="~/lib/metisMenu/metisMenu.js"></script>
    <script src="~/js/sb-admin-2.js"></script>
    <script src="~/js/jazure/crypto-min.js"></script>
    <script src="~/js/jazure/jazure.core/jazure.core.js"></script>
    <script src="~/js/jazure/jazure.storage/jazure.storage.core.js"></script>
    <script src="~/js/jazure/jazure.storage/jazure.storage.account.js"></script>
    <script src="~/js/jazure/jazure.storage/blob/jazure.storage.container.js"></script>
    <script src="~/js/jazure/jazure.storage/blob/jazure.storage.blob.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/azurespeed/common.js" asp-append-version="true"></script>
    <script src="~/js/azurespeed/utils.js" asp-append-version="true"></script>
    <script src="~/js/azurespeed/controllers/AzureBillingMetersController.js" asp-append-version="true"></script>
    <script src="~/js/azurespeed/controllers/AzureVMPricingController.js" asp-append-version="true"></script>
    <script src="~/js/azurespeed/controllers/CloudRegionFinderController.js" asp-append-version="true"></script>
    <script src="~/js/azurespeed/controllers/DownloadController.js" asp-append-version="true"></script>
    <script src="~/js/azurespeed/controllers/IpRangeController.js" asp-append-version="true"></script>
    <script src="~/js/azurespeed/controllers/LatencyController.js" asp-append-version="true"></script>
    <script src="~/js/azurespeed/controllers/MainController.js" asp-append-version="true"></script>
    <script src="~/js/azurespeed/controllers/UploadController.js" asp-append-version="true"></script>
    <script src="~/js/azurespeed/controllers/UploadLargeFileController.js" asp-append-version="true"></script>
  </environment>
  <environment exclude="Development">
    <script src="~/js/lib.all.min.js"></script>
    <script src="~/js/jazure.all.min.js" asp-append-version="true"></script>
    <script src="~/js/azurespeed.all.js" asp-append-version="true"></script>
  </environment>

  @RenderSection("scripts", required: false)
</body>
</html>
